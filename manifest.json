{
  "name": "Easy Json",
  "description": "A Json Chrome Extension,display json well,support BigInt,JsonPath.",
  "version": "1.0.2",
  "manifest_version": 3,
  "action": {
    "default_popup": "pages/setting.html",
    "default_icon": "images/easyjson.jpg"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/json-viewer/jsonviewer.js",
        "scripts/json-path/jsonpath.js",
        "scripts/json-bigint/bignumber.js",
        "scripts/json-bigint/parse.js",
        "scripts/easyjson.js",
        "scripts/content.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
