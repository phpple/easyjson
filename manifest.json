{
  "name": "Easy Json",
  "description": "A Json Chrome Extension,display json well,support BigInt,JsonPath.",
  "version": "1.1.3",
  "manifest_version": 3,
  "action": {
    "default_popup": "pages/setting.html",
    "default_icon": "icons/tinyicon.png"
  },
  "icons": {
    "128": "icons/tinyicon.png"
  },
  "permissions": ["scripting", "webNavigation"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "host_permissions": ["http://*/*", "https://*/*"]
}
